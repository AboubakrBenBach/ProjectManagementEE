<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <servlet>
    <servlet-name>ClientServlet</servlet-name>
    <servlet-class>com.learningee.controller.ClientServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ListeClients</servlet-name>
    <servlet-class>com.learningee.controller.ListeClients</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>SuppressionClient</servlet-name>
    <servlet-class>com.learningee.controller.SuppressionClient</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>CommandeServlet</servlet-name>
    <servlet-class>com.learningee.controller.CommandeServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ListeCommandes</servlet-name>
    <servlet-class>com.learningee.controller.ListeCommandes</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>SuppressionCommande</servlet-name>
    <servlet-class>com.learningee.controller.SuppressionCommande</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ClientServlet</servlet-name>
    <url-pattern>/inscriptionclient</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ListeClients</servlet-name>
    <url-pattern>/listeClients</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SuppressionClient</servlet-name>
    <url-pattern>/suppressionClient</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CommandeServlet</servlet-name>
    <url-pattern>/inscriptionCommande</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ListeCommandes</servlet-name>
    <url-pattern>/listeCommandes</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SuppressionCommande</servlet-name>
    <url-pattern>/suppressionCommande</url-pattern>
  </servlet-mapping>
</web-app>